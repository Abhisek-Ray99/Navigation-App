{
  "expo": {
    "name": "navigation-app",
    "slug": "navigation-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/img/playstore.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FF7078"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ab.navapp",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "com.ab.navapp",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/img/playstore.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
